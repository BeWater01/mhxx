  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="金火龙"><image class="iconImage" src="../../image/金火龙/1.png"></image>金火龙</button>
    <button class='button' bindtap='buttonTap' data-text="银火龙"><image class="iconImage" src="../../image/银火龙/1.png"></image>银火龙</button>
