  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="斩龙"><image class="iconImage" src="../../image/斩龙/1.png"></image>斩龙</button>
    <button class='button' bindtap='buttonTap' data-text="烬灭刃斩龙"><image class="iconImage" src="../../image/烬灭刃斩龙/1.png"></image>烬灭刃斩龙</button>
  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="迅龙"><image class="iconImage" src="../../image/迅龙/1.png"></image>迅龙</button>
