    <button class='button' bindtap='buttonTap' data-text="老山龙"><image class="iconImage" src="../../image/老山龙/1.png"></image>老山龙</button>
  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="岚龙"><image class="iconImage" src="../../image/岚龙/1.png"></image>岚龙</button>
    <button class='button' bindtap='buttonTap' data-text="骸龙"><image class="iconImage" src="../../image/骸龙/1.png"></image>骸龙</button>
  </view>
  <modal title="{{modalTitle}}" hidden="{{modalHidden}}" bindconfirm="modalConfirm" no-cancel class="model">
        <image class="image" src="../../image/{{modalTitle}}/2.png" style="width: 100%;" mode="widthFix"></image>
        <image class="image" src="../../image/{{modalTitle}}/3.png" style="width: 100%;" mode="widthFix"></image>
  </modal>
</view>

    

