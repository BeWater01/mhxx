  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="恐暴龙"><image class="iconImage" src="../../image/恐暴龙/1.png"></image>恐暴龙</button>
    <button class='button' bindtap='buttonTap' data-text="怒食恐暴龙"><image class="iconImage" src="../../image/怒食恐暴龙/1.png"></image>怒食恐暴龙</button>
  </view>
  <view class="btnSet">
