  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="轰龙"><image class="iconImage" src="../../image/轰龙/1.png"></image>轰龙</button>
    <button class='button' bindtap='buttonTap' data-text="荒勾爪轰龙"><image class="iconImage" src="../../image/荒勾爪轰龙/1.png"></image>荒勾爪轰龙</button>
  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="千刃龙"><image class="iconImage" src="../../image/千刃龙/1.png"></image>千刃龙</button>
    <button class='button' bindtap='buttonTap' data-text="麒麟"><image class="iconImage" src="../../image/麒麟/1.png"></image>麒麟</button>
  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="电龙"><image class="iconImage" src="../../image/电龙/1.png"></image>电龙</button>
