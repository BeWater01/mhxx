    <button class='button' bindtap='buttonTap' data-text="天慧龙"><image class="iconImage" src="../../image/天慧龙/1.png"></image>天慧龙</button>
  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="钢龙"><image class="iconImage" src="../../image/钢龙/1.png"></image>钢龙</button>
