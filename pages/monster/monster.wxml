  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="冰牙龙"><image class="iconImage" src="../../image/冰牙龙/1.png"></image>冰牙龙</button>
    <button class='button' bindtap='buttonTap' data-text="炎弋龙"><image class="iconImage" src="../../image/炎弋龙/1.png"></image>炎弋龙</button>
  </view>
  <view class="btnSet">
