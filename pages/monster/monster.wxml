  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="将军镰蟹"><image class="iconImage" src="../../image/将军镰蟹/1.png"></image>将军镰蟹</button>
    <button class='button' bindtap='buttonTap' data-text="铠裂将军蟹"><image class="iconImage" src="../../image/铠裂将军蟹/1.png"></image>铠裂将军蟹</button>
