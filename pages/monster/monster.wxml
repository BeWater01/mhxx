  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="水龙"><image class="iconImage" src="../../image/水龙/1.png"></image>水龙</button>
    <button class='button' bindtap='buttonTap' data-text="熔岩龙"><image class="iconImage" src="../../image/熔岩龙/1.png"></image>熔岩龙</button>
  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="黑狼鸟"><image class="iconImage" src="../../image/黑狼鸟/1.png"></image>黑狼鸟</button>
    <button class='button' bindtap='buttonTap' data-text="只眼黑狼鸟"><image class="iconImage" src="../../image/只眼黑狼鸟/1.png"></image>只眼黑狼鸟</button>
  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="大雪主白兔"><image class="iconImage" src="../../image/大雪主白兔/1.png"></image>大雪主白兔</button>
    <button class='button' bindtap='buttonTap' data-text="红盔青熊兽"><image class="iconImage" src="../../image/红盔青熊兽/1.png"></image>红盔青熊兽</button>
  </view>
  <view class="btnSet">
    <button class='button' bindtap='buttonTap' data-text="金狮子"><image class="iconImage" src="../../image/金狮子/1.png"></image>金狮子</button>
    <button class='button' bindtap='buttonTap' data-text="激昂金狮子"><image class="iconImage" src="../../image/激昂金狮子/1.png"></image>激昂金狮子</button>
